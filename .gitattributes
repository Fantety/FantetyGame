# Normalize EOL for all files that Git considers text files.
* text=auto eol=lf
asset/font filter=lfs diff=lfs merge=lfs -text
asset/img filter=lfs diff=lfs merge=lfs -text
asset/player filter=lfs diff=lfs merge=lfs -text
asset/sound filter=lfs diff=lfs merge=lfs -text
asset/ui filter=lfs diff=lfs merge=lfs -text
asset/Android filter=lfs diff=lfs merge=lfs -text
Fantetyx.png filter=lfs diff=lfs merge=lfs -text
icon.png filter=lfs diff=lfs merge=lfs -text
*.png filter=lfs diff=lfs merge=lfs -text
